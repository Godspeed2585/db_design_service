<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wk.warehouse.mapper.StationMapper">

    <!-- 查询所有站点 -->
    <select id="findAllStation" resultType="Station">
        select * from "TicketManagement".station
    </select>

    <insert id="insert"  parameterType="Station">
        insert * from "TicketManagement".station where "stationName" = #{stationName};
    </insert>

    <delete id="delete"  parameterType="Station">
        insert * from "TicketManagement".station where "stationName" = #{stationName};
    </delete>


</mapper>